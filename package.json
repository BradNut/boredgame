		"postcss-preset-env": "^8.5.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.1.0",
		"sass": "^1.64.1",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "^5.0.4",
		"svelte-sequential-preprocessor": "^2.0.1",
