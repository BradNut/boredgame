		"eslint-plugin-svelte": "^2.35.1",
		"just-clone": "^6.2.0",
		"just-debounce-it": "^3.2.0",
		"postcss": "^8.4.32",
		"postcss-import": "^15.1.0",
		"postcss-load-config": "^4.0.2",
		"postcss-preset-env": "^9.3.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.7.0",
		"sass": "^1.65.1",
